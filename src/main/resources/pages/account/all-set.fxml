<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox fx:id="vBoxFocus" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="768.0" minWidth="1024.0" prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ryanhuii.tuitionfinder.scene_controllers.account.AllSetController">
   <children>
      <HBox alignment="CENTER" spacing="2.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/logo.png" />
               </image>
            </ImageView>
            <Label text="TuitionFinder">
               <font>
                  <Font name="NiveauGroteskBold" size="32.0" />
               </font>
            </Label>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets bottom="150.0" top="40.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="confirmationMsg" text="You're all set!">
         <font>
            <Font name="NiveauGroteskBold" size="48.0" />
         </font>
         <VBox.margin>
            <Insets bottom="36.0" />
         </VBox.margin>
      </Label>
      <ImageView fitHeight="200.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/all_set.png" />
         </image>
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </ImageView>
      <Button mnemonicParsing="false" onAction="#onLetsGo" text="Let's go!" />
   </children>
</VBox>
